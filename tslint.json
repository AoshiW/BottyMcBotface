{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "no-empty-interface": false,
        "no-console": false,
        "forin": false,
        "arrow-parens": false,
        "max-classes-per-file": false,
        "array-type": {
            "options": [
                "array"
            ]
        },
        "object-literal-sort-keys": false,
        "curly": {
            "options": [
                "ignore-same-line"
            ]
        },
        "max-line-length": {
            "options": [
                280
            ]
        }
    },
    "rulesDirectory": []
}